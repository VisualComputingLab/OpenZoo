<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Servers</servlet-name>
        <servlet-class>gr.iti.openzoo.ui.servlets.ServersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Repository</servlet-name>
        <servlet-class>gr.iti.openzoo.ui.servlets.RepositoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Topologies</servlet-name>
        <servlet-class>gr.iti.openzoo.ui.servlets.TopologiesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DrawTopology</servlet-name>
        <servlet-class>gr.iti.openzoo.ui.servlets.DrawTopologyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessTopology</servlet-name>
        <servlet-class>gr.iti.openzoo.ui.servlets.ProcessTopologyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KeyValueServlet</servlet-name>
        <servlet-class>gr.iti.openzoo.ui.servlets.KeyValueServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainServlet</servlet-name>
        <servlet-class>gr.iti.openzoo.ui.servlets.MainServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servers</servlet-name>
        <url-pattern>/Servers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Repository</servlet-name>
        <url-pattern>/Repository</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Topologies</servlet-name>
        <url-pattern>/Topologies</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DrawTopology</servlet-name>
        <url-pattern>/DrawTopology</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessTopology</servlet-name>
        <url-pattern>/ProcessTopology</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KeyValueServlet</servlet-name>
        <url-pattern>/KeyValueServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainServlet</servlet-name>
        <url-pattern>/Main</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
